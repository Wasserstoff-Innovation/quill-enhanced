{
  "name": "@wasserstoff/quill-enhanced",
  "version": "0.1.0",
  "description": "A powerful, feature-rich WYSIWYG editor SDK built on top of Quill",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "demo": "vite examples",
    "demo:build": "vite build examples",
    "demo:preview": "vite preview examples"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "diff": "^5.1.0",
    "docx": "^8.5.0",
    "dompurify": "^3.0.6",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "marked": "^15.0.12",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/diff": "^5.0.3",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/quill": "^2.0.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "keywords": [
    "react",
    "typescript",
    "editor",
    "wysiwyg",
    "quill",
    "rich-text"
  ],
  "author": "Wasserstoff",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wasserstoff/quill-enhanced"
  },
  "bugs": {
    "url": "https://github.com/wasserstoff/quill-enhanced/issues"
  },
  "homepage": "https://github.com/wasserstoff/quill-enhanced#readme",
  "sideEffects": false
}
