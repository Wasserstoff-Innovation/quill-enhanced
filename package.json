{
  "name": "@wasserstoff/quill-enhanced",
  "version": "1.0.4",
  "description": "A powerful, feature-rich WYSIWYG editor SDK built on top of Quill",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "cd examples && npm run dev",
    "build": "tsup && npm run build:types",
    "build:types": "echo 'Type definitions manually created'",
    "build:watch": "tsup --watch",
    "build:analyze": "tsup --metafile",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "demo": "cd examples && npm run dev",
    "demo:build": "cd examples && npm run build",
    "demo:preview": "cd examples && npm run preview",
    "prepublishOnly": "npm run build",
    "release": "npm run prepublishOnly && npm publish"
  },
  "peerDependencies": {
    "quill": ">=1.3.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@types/diff-match-patch": "^1.0.36",
    "diff": "^5.1.0",
    "diff-match-patch": "^1.0.5",
    "docx": "^8.5.0",
    "dompurify": "^3.0.6",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "htmldiff-js": "^1.0.5",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.511.0",
    "marked": "^15.0.12",
    "react-icons": "^5.5.0",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/diff": "^5.0.3",
    "@types/dompurify": "^3.0.5",
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20.10.4",
    "@types/quill": "^2.0.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/turndown": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.4.32",
    "quill": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.2.2",
    "vite": "^6.3.5"
  },
  "keywords": [
    "react",
    "typescript",
    "editor",
    "wysiwyg",
    "quill",
    "rich-text"
  ],
  "author": "Wasserstoff",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wasserstoff-Innovation/quill-enhanced"
  },
  "bugs": {
    "url": "https://github.com/Wasserstoff-Innovation/quill-enhanced/issues"
  },
  "homepage": "https://github.com/Wasserstoff-Innovation/quill-enhanced#readme",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
